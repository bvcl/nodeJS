<script type="text/javascript">
var margin = {top:20,left:20,bottom:20,right:20};
var totalWidth = 540;
var totalHeight = 350;
var width = totalWidth - margin.left - margin.right;
var height = totalHeight - margin.top - margin.bottom;
var painel = d3.select("body")
.append("svg")
.attr("width", totalWidth)
.attr("height", totalHeight)

var svg = painel.append("g")
.attr("transform","translate("+margin.left+","+margin.top+")");

var dataMasc = [[1990,10.74],[1991,13.75],[1992,16.49],[1993,18.04],[1994,19.2],[1995,20.83],[1996,22.66],[1997,23.38],[1998,24.95],[1999,22.01],[2000,22.75],[2001,22.66],
                [2002,26.19],[2003,25.54],[2004,24.33],[2005,23.22],[2006,21.93],[2007,22.03],[2008,22.27]]

var dataFem = [[1990,1.96],[1991,2.87],[1992,4.07],[1993,5.1],[1994,5.86],[1995,7.45],[1996,9.32],[1997,11.12],[1998,12.69],[1999,11.96],[2000,13.05],[2001,13.81],
               [2002,16.85],[2003,16.55],[2004,15.89],[2005,15.5],[2006,14.48],[2007,13.93],[2008,14.24]]

 var xScale = d3.scaleLinear().domain([1990,2008]).range([0,width]);
 var yScale = d3.scaleLinear().domain([0,30]).range([height,0]);

 var line = d3.line()
              .x(function(d) {
                return xScale(d[0]);
              })
              .y(function(d) {
                return yScale(d[1]);
              })

 var xAxisGroup = svg.append("g").style("stroke-width",2).attr("transform","translate(0,310)").style("font", "16px times");
 var xAxis = d3.axisBottom(xScale).ticks(9).tickSize(-6).tickPadding(8).tickFormat(d3.format("d"));
 xAxis(xAxisGroup);

 var xAxisGroup2 = svg.append("g").style("stroke-width",2);
 var xAxis2 = d3.axisBottom(xScale).ticks(9).tickFormat("");
 xAxis2(xAxisGroup2);

 var yAxisGroup = svg.append("g").style("stroke-width",2).style("font", "14px times");
 var yAxis = d3.axisLeft(yScale).ticks(6).tickSize(-6);
 yAxis(yAxisGroup);

 var yAxisGroup2 = svg.append("g").style("stroke-width",2).attr("transform","translate(500,0)");
 var yAxis2 = d3.axisLeft(yScale).ticks(6).tickFormat("");
 yAxis2(yAxisGroup2);

 svg.append("path").attr("d",line(dataMasc)).style("fill", "none").style("stroke", "blue").style("stroke-width", 2) ;
 svg.append("path").attr("d",line(dataFem)).style("fill", "none").style("stroke", "red").style("stroke-width", 2) ;

</script>
